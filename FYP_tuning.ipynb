        "\n",
        "print(\"Examples of confusion between the two classes:\\n\")\n",
        "print(mis_filtered.head(20).to_string(index=False), \"\\n\")\n",
        "\n",
        "# --------------------------------------------------\n",
        "# 8. Interpretation\n",
